def CalculFrequence():
    with open("/Users/wandrille/Desktop/Programation/Python/Advent of Code/input.txt",'r') as s:
        i, freq=0, 0
        for line in s:
            if line[0]=='+':
                freq+=int(line[1:])
            elif line[0]=='-':
                freq-=int(line[1:])
    return freq
